{
  "name": "haskell-http-debug",
  "description": "Debug your http!",
  "logo": "https://halcyon.sh/logo.svg",
  "repository": "https://github.com/adzeitor/http-debug",
  "env": {
    "BUILDPACK_URL": {
      "description": "Heroku buildpack for deploying Haskell apps",
      "value": "https://github.com/mietek/haskell-on-heroku"
    },
    "HALCYON_NO_BUILD_DEPENDENCIES": {
      "description": "build dependencies",
      "value" : 0,
      "required":true
    }
    "MAX_MESSAGES": {
      "description": "Miximum number of messages in memory",
      "value" : "128",
      "required" : false
    }
  }
}
